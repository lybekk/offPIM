{
  "name": "offpim",
  "version": "0.3.0",
  "private": true,
  "description": "Personal Information Manager using PouchDB/CouchDB",
  "author": "Christoffer Lybekk",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "dist": "vue-cli-service electron:build --publish=never",
    "pack": "vue-cli-service electron:build --dir",
    "lint": "vue-cli-service lint",
    "docs:build": "bundle exec jekyll build",
    "docs:dev": "bundle exec jekyll serve",
    "electron:build": "vue-cli-service electron:build",
    "electron:generate-icons": "electron-icon-builder --input=./public/icon.png --output=build --flatten",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "main": "background.js",
  "dependencies": {
    "chart.js": "^2.9.3",
    "core-js": "^3.6.5",
    "debug": "^4.2.0",
    "leaflet": "^1.6.0",
    "lunr": "^2.3.8",
    "nano": "^8.2.2",
    "pouchdb": "^7.2.1",
    "pouchdb-authentication": "^1.1.3",
    "pouchdb-browser": "^7.2.1",
    "pouchdb-debug": "^7.2.1",
    "pouchdb-find": "^7.2.1",
    "pouchdb-live-find": "^0.4.0",
    "register-service-worker": "^1.7.1",
    "vue": "^2.6.10",
    "vue-chartjs": "^3.5.0",
    "vue-router": "^3.3.2",
    "vuetify": "^2.2.34",
    "vuex": "^3.3.0"
  },
  "devDependencies": {
    "@mdi/font": "^4.8.95",
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-plugin-eslint": "^4.3.1",
    "@vue/cli-plugin-pwa": "^4.3.1",
    "@vue/cli-plugin-router": "^4.3.1",
    "@vue/cli-plugin-vuex": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "babel-eslint": "^10.1.0",
    "electron": "^8.2.4",
    "electron-icon-builder": "^1.0.2",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.14.0",
    "pug": "^2.0.4",
    "pug-plain-loader": "^1.0.0",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "vue-cli-plugin-electron-builder": "^1.4.6",
    "vue-cli-plugin-vuetify": "^2.0.5",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.3.0"
